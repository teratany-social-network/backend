                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "pageId": {
                                    "type": "string",
                                    "description": "Identifiant de la page",
                                    "example": "6528f7d0ec2defa28afe1ee4"
                                },
                                "category": {
                                    "type": "string",
                                    "description": "Les nouvelles catégorie de la page",
                                    "example": "adf"
                                }
                            }
                        }
                    }
                }
