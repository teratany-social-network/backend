                                "phoneNumber": {
                                    "type": "string"
                                },
                                "website": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "Mot de passe modifié"
                }
            }
        }
    },
    "/pages/walletId/": {
        "patch": {
            "tags": [
                "pages"
            ],
            "summary": "Modifier le wallet deviantcoin d'une page",
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "pageId": {
                                    "type": "string",
                                    "description": "Identifiant de la page",
                                    "example": "6528f7d0ec2defa28afe1ee4"
                                },
                                "deviantWalletID": {
                                    "type": "string",
                                    "description": "Le nouveau deviantWalletID",
                                    "example": "dSSPDMr8JqF7ms6zVVLzpKXUQsB2k4yCzW"
                                }
                            }
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "Mot de passe modifié"
                }
            }
        }
    },
    "/pages/coordonates/": {
        "patch": {
            "tags": [
                "pages"
            ],
            "summary": "Modifier les coordonées géographiques (latitude, longitude) d'une page",
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "pageId": {
                                    "type": "string",
                                    "description": "Identifiant de la page",
                                    "example": "6528f7d0ec2defa28afe1ee4"
                                },
                                "latitude": {
                                    "type": "number",
                                    "example": 18.754567
                                },
                                "longitude": {
                                    "type": "number",
                                    "example": -27.58572
                                }
                            }
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "Mot de passe modifié"
                }
            }
        }
    }
}