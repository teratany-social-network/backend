    "/pages/address/": {
        "patch": {
            "tags": [
                "pages"
            ],
            "summary": "Modifier l'adresse et le pays d'origine d'une page",
            "requestBody": {
                "required": true,
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "pageId": {
                                    "type": "string",
                                    "description": "Identifiant de la page",
                                    "example": "6528f7d0ec2defa28afe1ee4"
                                },
                                "address": {
                                    "type": "string",
                                    "description": "La nouvelle adresse de la page",
                                    "example": "Analakely, Antananarivo"
                                },
                                "country": {
                                    "type": "string",
                                    "description": "Le nouveau de la page",
                                    "example": "Madagascar"
                                }
                            }
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "Mot de passe modifié"
                }
            }
        }
    },
    "/pages/contact/": {
        "patch": {
            "tags": [
